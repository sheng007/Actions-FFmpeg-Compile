+static DRAFFT* DRAFFTInit(unsigned short n)
+{
+	DRAFFT* fft = (DRAFFT*)malloc(sizeof(DRAFFT));
+	fft->n = n;
+	fft->work = (complex_t*)malloc(n * sizeof(complex_t));
+	fft->tab = (complex_t*)malloc(n * sizeof(complex_t));
+	init(n, fft->tab, fft->ifac);
+	return fft;
+}
