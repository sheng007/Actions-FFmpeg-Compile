+int
+cavs_cpu_num_processors (void)
+{
+#if SYS_WINDOWS
+  return pthread_num_processors_np ();
+
+#elif SYS_LINUX
+  unsigned int bit;
+  int np;
+  cpu_set_t p_aff;
+  memset (&p_aff, 0, sizeof (p_aff));
+  sched_getaffinity (0, sizeof (p_aff), &p_aff);
+  for (np = 0, bit = 0; bit < sizeof (p_aff); bit++)
+    np += (((unsigned char *) & p_aff)[bit / 8] >> (bit % 8)) & 1;
+  return np;
+
+#else
+  return 1;
+#endif
+}
