+int cavs_biari_decode_stuffing_bit(cavs_cabac_t *cb)
+{
+    bi_ctx_t ctx;
+    uint32_t bit;
+	ctx.cycno = 0;
+    ctx.lg_pmps = 1 << LG_PMPS_SHIFTNO;
+    ctx.mps = 0;
+    bit = cavs_biari_decode_symbol(cb, &ctx);
+    
+    return bit;
+}
