+/****************************************************************************
+ * cavs_realloc:
+ ****************************************************************************/
+void *cavs_realloc( void *p, int i_size )
+{
+    int       i_old_size = 0;
+    uint8_t * p_new;
+    if( p )
+    {
+        i_old_size = *( (int*) ( (uint8_t*) p  - sizeof( void ** ) -
+                     sizeof( int ) ) );
+    }
+    p_new = (uint8_t *)cavs_malloc( i_size );
+    if( i_old_size > 0 && i_size > 0 )
+    {
+        memcpy( p_new, p, ( i_old_size < i_size ) ? i_old_size : i_size );
+    }
+    cavs_free( p );
+
+    return p_new;
+}
