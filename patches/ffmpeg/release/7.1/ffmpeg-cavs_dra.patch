+static inline int cavs_clip(int a, int amin, int amax)
+{
+     if (a < amin)      return amin;
+     else if (a > amax) return amax;
+     else               return a;
+}
+static inline uint8_t cavs_clip_uint8(int a)
+{
+     if (a&(~255)) return (-a)>>31;
+     else          return a;
+}
