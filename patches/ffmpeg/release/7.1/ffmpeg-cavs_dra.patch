+        p->bs_read[SYNTAX_INTRA_CHROMA_PRED_MODE] = cavs_cavlc_get_ue;
+        p->bs_read[SYNTAX_INTRA_CBP] = cavs_cavlc_get_intra_cbp;
+        p->bs_read[SYNTAX_INTER_CBP] = cavs_cavlc_get_inter_cbp;
