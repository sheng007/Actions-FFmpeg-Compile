+	{
+		return -1;
+	}
+	else if (nResult < -1)
+	{
+		ClearBits(bs, pBook->l0);
+		int nIndex1 = ShowBits(bs, pBook->l1);
+		int t = nResult + 2;
+		nResult = pBook->l1_items[-t][nIndex1];
+		if (nResult == -1)
+		{
+			return -1;
+		}
+	}
+	ClearBits(bs, nResult >> DRA_S_BIT);
+	nResult = nResult & DRA_S_BIT_MASK;
+	return nResult;
+}
