+static void copy_mvs(cavs_vector *mv, enum cavs_block_size size) 
+{
+    switch(size)
+    {
+    case BLK_16X16:
+        mv[MV_STRIDE  ] = mv[0];
+        mv[MV_STRIDE+1] = mv[0];
+    case BLK_16X8:
+        mv[1] = mv[0];
+        break;
+    case BLK_8X16:
+        mv[MV_STRIDE] = mv[0];
+        break;
+    default:
+    	break;
+    }
+}
