+#if TEST_CABAC
+       t_rlps = (s_flag==0)? lg_pmps_shift2[lg_pmps]:(cb->t1 + lg_pmps_shift2[lg_pmps]);	
+#else
+    	t_rlps = (s_flag==0)? (lg_pmps>>LG_PMPS_SHIFTNO):(cb->t1 + (lg_pmps>>LG_PMPS_SHIFTNO));		
+#endif
