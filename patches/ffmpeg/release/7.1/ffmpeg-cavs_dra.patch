+
+        switch( i_startcode )
+        {	
+            case CAVS_VIDEO_SEQUENCE_START_CODE:
+                cavs_bitstream_restore( p, field );
+                //cavs_free(m_buf);
+                return i_startcode;
+            case CAVS_VIDEO_SEQUENCE_END_CODE:
