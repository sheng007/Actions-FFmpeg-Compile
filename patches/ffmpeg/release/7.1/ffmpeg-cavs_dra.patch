+/* cavs_botfield_pack */
+uint32_t cavs_botfield_pack( InputStream*p, frame_pack *field )
+{
+    uint32_t i_startcode;
+    int i_len;
+    //unsigned char *m_buf;
+
+    //m_buf = (unsigned char *)cavs_malloc((MAX_CODED_FRAME_SIZE/2)*sizeof(unsigned char));
