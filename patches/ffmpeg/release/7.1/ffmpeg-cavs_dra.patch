+    		table2D_inter_4x4=inter_2dvlc_4x4[i];
+    		for(run=0;run<16;run++)
+    			for(level=0;level<14;level++)
+    			{
+    				ipos=table2D_inter_4x4[run][level];
+    				assert(ipos<40);
