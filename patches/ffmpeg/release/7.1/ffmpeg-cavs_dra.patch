+	{
+		return -1;
+	}
+	return (nIndex + nDiff) % pBook->size;
+}
+
+static int DRAHuffmanDecodeRecursive(DRABitstream* bs, const DRAHuffmanCodebook* pBook)
+{
+	int k = -1;
+	int nIndex = -1;
+	do
+	{
