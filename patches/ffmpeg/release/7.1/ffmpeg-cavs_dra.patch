+    						AVS_2DVLC_INTER_dec_4x4[i][ipos][1]=run;
+
+    						AVS_2DVLC_INTER_dec_4x4[i][ipos+1][0]=-(level+1);
+    						AVS_2DVLC_INTER_dec_4x4[i][ipos+1][1]=run;
+    					}
