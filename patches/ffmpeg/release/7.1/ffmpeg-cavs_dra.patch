+static inline void scale_mv(cavs_decoder *p, int *d_x, int *d_y, cavs_vector *src, int distp) 
+{
+
+    int den;
+
+    if(abs(src->ref) > 3)
+    {
+    	p->b_error_flag = 1;
+    	return;
+    }
+
+    den = p->i_scale_den[src->ref];
+
+    *d_x = (src->x*distp*den + 256 + (src->x>>31)) >> 9;
+    *d_y = (src->y*distp*den + 256 + (src->y>>31)) >> 9;
+}
